{
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": [
      "category"
    ]
  },
  "plugins": [
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/category"
  ],
  "source": {
    "include": [
      "packages/app-router/src",
      "packages/identityprovider/src"
    ],
    "includePattern": ".+\\.ts$",
    "excludePattern": ".+\\.spec.ts$"
  }
}