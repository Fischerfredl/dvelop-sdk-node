{
    "roots": [
        "packages"
    ],
    "testMatch": [
        "**/*.spec.ts"
    ],
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
        "**/src/**/*.ts",
        "!**/src/index.ts"
    ],
    "coverageReporters": ["text"]

}